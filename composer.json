{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "friendsofsymfony/rest-bundle": "^2.2",
        "javiereguiluz/easyadmin-bundle": "^1.16",
        "nelmio/cors-bundle": "^1.5",
        "respect/validation": "^1.1",
        "samj/fractal-bundle": "^1.0",
        "symfony/console": "^3.3",
        "symfony/dotenv": "^3.3",
        "symfony/expression-language": "^3.3",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^3.3",
        "symfony/psr-http-message-bridge": "^1.0",
        "symfony/serializer": "^3.3",
        "symfony/yaml": "^3.3",
        "zendframework/zend-diactoros": "^1.4"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0",
        "deployer/deployer": "^5.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "make cache-warmup": "script",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BQTRGNK319VYR6C3XCZTZMAX",
            "allow-contrib": true
        }
    }
}
